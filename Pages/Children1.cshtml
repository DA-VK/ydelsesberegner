@page
@model Ydelsesberegner.Pages.Children1Model
@{
    ViewData["Title"] = "Børn";
}

<div id="2" class="text-left">
   <br/><br/><br/>
   <h1>Har du hjemmeboende børn?</h1>
    <br/>
               
    <form method="post">
        @foreach (var svar in Model.Children1Svar)
        {
             <label><input type="radio" asp-for="Children1" id="Children1@(svar)" value="@svar" />@svar</label>
            <br/>
        }
        <br/> 
        <input type="button" value="Tilbage" onclick="history.back()" class="btn btn-secondary">
        <input type="submit" value="Næste" class="btn btn-secondary"/>
    </form>
    
       
        
 
</div>

